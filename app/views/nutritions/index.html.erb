<% breadcrumb :nutritions %>

<h1><%=t "activerecord.models.nutrition" %></h1>

<table>
  <thead>
    <tr>
      <th><%=t "activerecord.attributes.nutrition.name" %></th>
      <th><%=t "activerecord.attributes.nutrition.energy" %></th>
      <th><%=t "activerecord.attributes.nutrition.protein" %></th>
      <th><%=t "activerecord.attributes.nutrition.lipid" %></th>
      <th><%=t "activerecord.attributes.nutrition.carbohydrate" %></th>
      <th><%=t "activerecord.attributes.nutrition.dietary_fiber" %></th>
      <th><%=t "activerecord.attributes.nutrition.saturated_fatty_acid" %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nutritions.each do |nutrition| %>
      <tr>
        <td><%= nutrition.name %></td>
        <td><%= nutrition.energy %></td>
        <td><%= nutrition.protein %></td>
        <td><%= nutrition.lipid %></td>
        <td><%= nutrition.carbohydrate %></td>
        <td><%= nutrition.dietary_fiber %></td>
        <td><%= nutrition.saturated_fatty_acid %></td>
        <td><%= link_to 'Show', nutrition %></td>
        <td><%= link_to 'Edit', edit_nutrition_path(nutrition) %></td>
        <td><%= link_to 'Destroy', nutrition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nutrition', new_nutrition_path %>
